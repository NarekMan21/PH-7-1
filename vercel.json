{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "/save",
      "dest": "/api/index.js"
    },
    {
      "src": "/history",
      "dest": "/api/index.js"
    },
    {
      "src": "/export/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/stats",
      "dest": "/api/index.js"
    },
    {
      "src": "/clear",
      "dest": "/api/index.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
