{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "/save",
      "dest": "/api/index.js"
    },
    {
      "src": "/history",
      "dest": "/api/index.js"
    },
    {
      "src": "/export/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/stats",
      "dest": "/api/index.js"
    },
    {
      "src": "/clear",
      "dest": "/api/index.js",
      "methods": ["DELETE"]
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|ico|json))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
